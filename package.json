  "dependencies": {
    "@orca-so/common-sdk": "^0.1.0",
    "@orca-so/whirlpools-sdk": "^0.5.0",
    "@project-serum/anchor": "0.24.2",
    "@solana/spl-token": "0.1.8",
    "@solana/spl-token-swap": "0.1.2",
    "buffer-layout": "1.2.2",
    "decimal.js": "^10.3.1",
